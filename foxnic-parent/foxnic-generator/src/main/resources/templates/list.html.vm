 <!DOCTYPE html>
<html>

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta charset="utf-8"/>
    <title th:text="${lang.translate('#(topic)'#(languageContextPartHTML))}">#(topic)</title>
    <link th:if(theme.ico!="null") rel="shortcut icon" th:href="${theme.ico}" type="image/vnd.microsoft.icon">
    <link th:if(theme.ico!="null") rel="icon" th:href="theme.ico" type="image/vnd.microsoft.icon">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="/assets/libs/layui/css/layui.css" th:href="'/assets/libs/layui/css/layui.css?'+${cacheKey}"/>
    <link rel="stylesheet" href="/assets/css/admin.css" th:href="'/assets/css/admin.css?'+${cacheKey}"/>
    <link rel="stylesheet" href="/assets/libs/toast/css/toast.css" type="text/css" th:href="'/assets/libs/toast/css/toast.css?'+${cacheKey}">
    <link rel="stylesheet" href="/assets/css/foxnic-web.css" th:href="'/assets/css/foxnic-web.css?'+${cacheKey}">
    <link href="/assets/libs/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet"/>
    <link href="/assets/libs/material-design-webfont/css/material-design-icons-min.css" rel="stylesheet">
    #for(f : cssList)
    <link rel="stylesheet" href="#(f)" th:href="'#(f)?'+${cacheKey}"/>
    #end
    <script th:inline="javascript">var foxnic_cachekey=[[${cacheKey}]];</script>
    <style>
        #if(marginDisabled)
        .layui-card-body .layui-table {
            margin:0px;
        }
        #end
    </style>
    <link th:each="css:${theme.css}" rel="stylesheet" th:href="${css}+'?'+${cacheKey}">
</head>

<body style="overflow-y: hidden">

<div class="#if(!marginDisabled)layui-card#else layui-card-no-shadow #end">

    <div class="layui-card-body" style="#if(marginDisabled)padding:0px#end">

        <div class="search-bar" style="#if(searchDisabled) display: none; #end">

            <div class="search-input-rows" style="opacity: 0">
                #for(searchFields : searchRows)
                <!-- 搜索输入区域 -->
                <div class="layui-form toolbar search-inputs">
                    #for(f : searchFields)
                    <!-- #(f.labelInSearch) , #(f.varName) ,typeName=#(f.typeName), isHideInSearch=#(f.isHideInSearch) -->
                    #if(!f.isHideInSearch)
                    <div class="search-unit">
                        <div class="search-label-div" style="#if(f.search.labelWidth!=null)width:#(f.search.labelWidth)px#end"><span th:text="${lang.translate('#(f.labelInSearch)'#(languageContextPartHTML))}" class="search-label #(f.varName)-label">#(f.labelInSearch)</span><span class="search-colon">:</span></div>
                        #if(f.typeName.equals("logic_switch"))
                        <div id="#(f.varName)" data='[{"name":"#(f.logicField.labelOn)","value":"#(f.logicField.valueOn)"},{"name":"#(f.logicField.labelOff)","value":"#(f.logicField.valueOff)"}]' style="width:#(f.search.inputWidth)px"></div>
                        #else if(f.typeName.equals("select_box"))
                        #if(f.selectField.queryApi!=null && f.search.fuzzySearch)
                         <input id="#(f.varName)" class="layui-input search-input" style="width: #(f.search.inputWidth)px" type="text" />
                        #else
                        <div id="#(f.varName)" th:data="#if(f.selectField.enumTypeName!=null)${enum.toArray('#(f.selectField.enumTypeName)')}#end#if(f.selectField.queryApi!=null)${'#(f.selectField.queryApi)'}#end#if(f.selectField.dictCode!=null)${dict.toArray('#(f.selectField.dictCode)')}#end#if(f.selectField.varName!=null)${#(f.selectField.varName)}#end" style="width:#(f.search.inputWidth)px" extraParam="{}"></div>
                        #end
                        #else if(f.typeName.equals("radio_box"))

                        #if(f.radioField.dictCode!=null)
                        <div id="#(f.varName)" th:data="${dict.toArray('#(f.radioField.dictCode)')}" style="width:#(f.search.inputWidth)px"></div>
                        #end

                        #if(f.radioField.enumTypeName!=null)
                        <div id="#(f.varName)" th:data="${enum.toArray('#(f.radioField.enumTypeName)')}" style="width:#(f.search.inputWidth)px"></div>
                        #end
                        #if(f.radioField.varName!=null)
                            <div id="#(f.varName)" th:data="${#(f.radioField.varName)}" style="width:#(f.search.inputWidth)px"></div>
                        #end
                        #else if(f.typeName.equals("check_box"))

                            #if(f.checkField.dictCode!=null)
                                <div id="#(f.varName)" th:data="${dict.toArray('#(f.checkField.dictCode)')}" style="width:#(f.search.inputWidth)px"></div>
                            #end

                            #if(f.checkField.enumTypeName!=null)
                                <div id="#(f.varName)" th:data="${enum.toArray('#(f.checkField.enumTypeName)')}" style="width:#(f.search.inputWidth)px"></div>
                            #end

                            #if(f.checkField.varName!=null)
                                <div id="#(f.varName)" th:data="${#(f.checkField.varName)}" style="width:#(f.search.inputWidth)px"></div>
                            #end

                        #else if(f.typeName.equals("date_input"))
                            #if(f.search.searchInRange)
                            <input type="text" id="#(f.varName)-begin" style="width: #(f.search.inputWidth)px" lay-verify="date" th:placeholder="${lang.translate('开始日期'#(languageContextPartHTML))}" autocomplete="off" class="layui-input search-input search-date-input"  readonly >
                            <span class="search-dash">-</span>
                            <input type="text" id="#(f.varName)-end"  style="width: #(f.search.inputWidth)px"  lay-verify="date" th:placeholder="${lang.translate('结束日期'#(languageContextPartHTML))}" autocomplete="off" class="layui-input search-input search-date-input" readonly>
                            #else
                            <input type="text" id="#(f.varName)"  style="width: #(f.search.inputWidth)px"  lay-verify="date" th:placeholder="${lang.translate('请选择'#(languageContextPartHTML))}" autocomplete="off" class="layui-input search-input search-date-input" readonly>
                            #end
                        #else if(f.typeName.equals("number_input"))
                            #if(f.search.searchInRange)
                            <input id="#(f.varName)-begin" class="layui-input search-input" style="width: #(f.search.inputWidth)px" type="text" autocomplete="off" input-type="number_input"  integer="#(f.numberInput.isInteger)" decimal="#(f.numberInput.isDecimal)" allow-negative="#(f.numberInput.allowNegative)" step="#(f.numberInput.step)" min-value="#(f.numberInput.minValue)" max-value="#(f.numberInput.maxValue)" scale="#(f.numberInput.scale)"/>
                            <span class="search-dash">-</span>
                            <input id="#(f.varName)-end" class="layui-input search-input" style="width: #(f.search.inputWidth)px" type="text" autocomplete="off" input-type="number_input"  integer="#(f.numberInput.isInteger)" decimal="#(f.numberInput.isDecimal)" allow-negative="#(f.numberInput.allowNegative)" step="#(f.numberInput.step)" min-value="#(f.numberInput.minValue)" max-value="#(f.numberInput.maxValue)" scale="#(f.numberInput.scale)"/>
                            #else
                            <input id="#(f.varName)" class="layui-input search-input" style="width: #(f.search.inputWidth)px" type="text" autocomplete="off" input-type="number_input" integer="#(f.numberInput.isInteger)" decimal="#(f.numberInput.isDecimal)" allow-negative="#(f.numberInput.allowNegative)" step="#(f.numberInput.step)" min-value="#(f.numberInput.minValue)" max-value="#(f.numberInput.maxValue)" scale="#(f.numberInput.scale)"/>
                            #end
                        #else if(f.typeName.equals("button"))
                            <input lay-filter="#(f.varName)" id="#(f.varName)" name="#(f.varName)"  type="hidden" class="layui-input"  #if(f.validate!=null)  lay-verify="#if(f.validate.isRequired)|required#end#if(f.validate.isPhone)|phone#end#if(f.validate.isEmail)|email#end#if(f.validate.isUrl)|url#end#if(f.validate.isDate)|date#end#if(f.validate.isIdentity)|identity#end"  #end  />
                            <button id="#(f.varName)-button" type="button" notExistAction-type="#(f.buttonField.notExistAction.actionType)" class="layui-btn layui-btn-primary #if(f.buttonField.notExistAction.css!=null) #(f.buttonField.notExistAction.css) #end" style="width: #(f.search.inputWidth)px">#if(f.buttonField.notExistAction.iconHtml!=null) #(f.buttonField.notExistAction.iconHtml) #end<span th:text="${lang.translate('#(f.buttonField.text)'#(languageContextPartHTML))}" th:default-label="${lang.translate('#(f.buttonField.text)'#(languageContextPartHTML))}">按钮文本</span></button>
                        #else
                        <input id="#(f.varName)" class="layui-input search-input" style="width: #(f.search.inputWidth)px" type="text" />
                        #end
                    </div>
                    #end
                    #end

                    #if(isTree)
                    <span id="tree-nav" class="layui-breadcrumb" style="padding-left:16px"></span>
                    #end

                </div>
                #end
            </div>


            <!-- 按钮区域 -->
            <div id="search-area" class="layui-form toolbar search-buttons" style="opacity: 0">
                <button id="search-button" class="layui-btn icon-btn"><i class="layui-icon">&#xe615;</i>&nbsp;&nbsp;<span th:text="${lang.translate('搜索','','cmp:table.search')}">搜索</span></button>
                #if(searchRows.size() > searchRowsDisplay)
                <button id="search-button-advance" class="layui-btn layui-btn-primary icon-btn search-button-advance"><i class="layui-icon">&#xe671;</i><span th:text="${lang.translate('更多','','cmp:table.search')}">更多</span></button>
                #end
            </div>
        </div>

        <div id="table-area" style="margin-top: #if(searchDisabled)0px #else #(searchRowsDisplay * 42)px #end">
            <table class="layui-table" id="data-table" lay-filter="data-table"></table>
        </div>

    </div>
</div>

<script type="text/javascript" src="/module/global.js" th:src="'/module/global.js?'+${cacheKey}"></script>
<script type="text/javascript" src="/assets/libs/jquery-3.2.1.min.js" th:src="'/assets/libs/jquery-3.2.1.min.js?'+${cacheKey}"></script>
<script type="text/javascript" src="/assets/libs/pandyle.min.js" th:src="'/assets/libs/pandyle.min.js?'+${cacheKey}"></script>
<script type="text/javascript" src="/assets/libs/layui/layui.js" th:src="'/assets/libs/layui/layui.js?'+${cacheKey}"></script>
<script type="text/javascript" src="/assets/libs/toast/js/toast.js" th:src="'/assets/libs/toast/js/toast.js?'+${cacheKey}"></script>
#for(f : jsList)
<script type="text/javascript" src="#(f)" th:src="'#(f)?'+${cacheKey}"></script>
#end
<!-- 表格工具栏 -->
<script type="text/html" id="toolbarTemplate">
    <div class="layui-btn-container">
        #if(!disableCreateNew)
        #if(bpm)
        <button th:if="${perm.checkAuth('#(authPrefix):create')}" id="add-button" class="layui-btn icon-btn layui-btn-sm create-new-button #if(createNewButton.css!=null)#(createNewButton.css)#end" lay-event="create"><i class="layui-icon">&#xe654;</i><span th:text="${lang.translate('发起审批','','cmp:table.button')}">发起审批</span></button>
        #else
        <button th:if="${perm.checkAuth('#(authPrefix):create')}" id="add-button" class="layui-btn icon-btn layui-btn-sm create-new-button #if(createNewButton.css!=null)#(createNewButton.css)#end" lay-event="create"><i class="layui-icon">&#xe654;</i><span th:text="${lang.translate(#if(createNewButton.label==null)'新建'#else'#(createNewButton.label)'#end,'','cmp:table.button')}">#if(createNewButton.label==null)新建#else#(createNewButton.label)#end</span></button>
        #end
        #end
        #if(!disableBatchDelete)
        #if(isSimplePK && !bpm)
        <button th:if="${perm.checkAuth('#(authPrefix):delete-by-ids')}" id="delete-button" class="layui-btn icon-btn layui-btn-danger layui-btn-sm batch-delete-button #if(batchDeleteButton.css!=null)#(batchDeleteButton.css)#end" lay-event="batch-del"><i class="layui-icon">&#xe67e;</i><span th:text="${lang.translate(#if(batchDeleteButton.label==null)'删除'#else'#(batchDeleteButton.label)'#end,'','cmp:table.button')}">#if(createNewButton.label==null)删除#else#(createNewButton.label)#end</span></button>
        #end
        #end
        #for(b:toolButtons)
        <button id="#(b.id)"  #if(b.perm!=null)th:if="${perm.checkAuth('#(b.perm)')}"#end class="layui-btn icon-btn layui-btn-sm #if(b.css!=null) #(b.css) #end" lay-event="tool-#(b.id)">#if(b.iconHtml!=null) #(b.iconHtml) #end<span th:text="${lang.translate('#(b.label)','','cmp:table.button')}">#(b.label)</span></button>
        #end
    </div>
</script>

<!-- 表格操作列 -->
<script type="text/html" id="tableOperationTemplate">
	#if(isTree)
    <button class="layui-btn layui-btn-primary layui-btn-xs" lay-event="drill" data-id="{{d.id}}"><span th:text="${lang.translate('下级','','cmp:table.ops')}">下级</span></button>
	#end

    #if(!disableFormView)
    #if(!bpm)
    <button th:if="${perm.checkAuth('#(authPrefix):view-form')}" class="layui-btn layui-btn-primary layui-btn-xs ops-view-button #if(formViewButton.css!=null)#(formViewButton.css)#end" lay-event="view"  data-id="{{d.id}}"> <span th:text="${lang.translate(#if(formViewButton.label==null)'查看'#else'#(formViewButton.label)'#end,'','cmp:table.ops')}">#if(formViewButton.label==null)查看#else#(formViewButton.label)#end</span></button>
    #end
    #end
    #if(!disableModify)
    #if(bpm)
    <button th:if="${perm.checkAnyAuth('#(authPrefix):update','#(authPrefix):save')}" class="layui-btn layui-btn-primary layui-btn-xs ops-edit-button #if(modifyButton.css!=null)#(modifyButton.css)#end" lay-event="edit"data-id="{{d.id}}"><span th:text="${lang.translate('打开','','cmp:table.ops')}">打开</span></button>
    #else
    <button th:if="${perm.checkAnyAuth('#(authPrefix):update','#(authPrefix):save')}" class="layui-btn layui-btn-primary layui-btn-xs ops-edit-button #if(modifyButton.css!=null)#(modifyButton.css)#end" lay-event="edit"data-id="{{d.id}}"><span th:text="${lang.translate(#if(modifyButton.label==null)'修改'#else'#(modifyButton.label)'#end,'','cmp:table.ops')}">#if(modifyButton.label==null)修改#else#(modifyButton.label)#end</span></button>
    #end
    #end


    #if(!disableSingleDelete)
    #if(bpm)
    <button th:if="${perm.checkAuth('#(authPrefix):delete')}" class="layui-btn layui-btn-xs layui-btn-danger ops-delete-button #if(deleteButton.css!=null)#(deleteButton.css)#end" lay-event="del" data-id="{{d.id}}"><span th:text="${lang.translate('废弃','','cmp:table.ops')}">废弃</span></button>
    #else
    <button th:if="${perm.checkAuth('#(authPrefix):delete')}" class="layui-btn layui-btn-xs layui-btn-danger ops-delete-button #if(deleteButton.css!=null)#(deleteButton.css)#end" lay-event="del" data-id="{{d.id}}"><span th:text="${lang.translate(#if(deleteButton.label==null)'删除'#else'#(deleteButton.label)'#end,'','cmp:table.ops')}">#if(deleteButton.label==null)删除#else#(deleteButton.label)#end</span></button>
    #end
    #end

    #for(b:opColumnButtons)
    <button #if(b.perm!=null)th:if="${perm.checkAuth('#(b.perm)')}"#end class="layui-btn layui-btn-xs #if(b.css!=null) #(b.css) #end" lay-event="#(b.id)" data-id="{{d.id}}">#if(b.iconHtml!=null) #(b.iconHtml) #end<span th:text="${lang.translate('#(b.label)','','cmp:table.ops')}">#(b.label)</span></button>
    #end

    #if(opColumnMenus!=null && opColumnMenus.size()>0)
    <button class="layui-btn layui-btn-xs layui-btn-primary layui-border-blue" lay-event="ops-more" data-id="{{d.id}}"><span th:text="${lang.translate('更多','','cmp:table.ops')}">更多</span><i class="layui-icon layui-icon-down"></i></button>
    #end
</script>

#if(hasLogicField)
<!-- 表格状态列 -->
#for(f:fields)
#if(f.typeName.equals("logic_switch"))
<script type="text/html" id="cell-tpl-#(f.varName)">
    <input type="checkbox" class="switch-#(f.varName) list-checkbox" lay-filter="cell-tpl-#(f.varName)" value="{{d.#(idPropertyName)}}" lay-skin="switch" lay-text="#(f.logicField.labelOn)|#(f.logicField.labelOff)&nbsp;"  {{d.#(f.varName)==#(f.logicField.valueOn)?'checked':''}} data-id="{{d.#(idPropertyName)}}"/>
</script>
#end
#end
#end

<script th:inline="javascript">
    var LAYUI_TABLE_WIDTH_CONFIG = [[${pageHelper.getTableColumnWidthConfig('data-table')}]];
    #for(f:fields)
    #if(f.typeName.equals("radio_box"))
    #if(f.radioField.dictCode!=null)
    var RADIO_#(f.varName.toUpperCase())_DATA = [[${dict.toArray('#(f.radioField.dictCode)')}]];
    #end
    #if(f.radioField.enumTypeName!=null)
    var RADIO_#(f.varName.toUpperCase())_DATA = [[${enum.toArray('#(f.radioField.enumTypeName)')}]];
    #end
    #if(f.radioField.varName!=null)
    var RADIO_#(f.varName.toUpperCase())_DATA = [[${#(f.radioField.varName)}]];
    #end
    #else if(f.typeName.equals("check_box"))
    #if(f.checkField.dictCode!=null)
    var CHECK_#(f.varName.toUpperCase())_DATA = [[${dict.toArray('#(f.checkField.dictCode)')}]];
    #end
    #if(f.checkField.enumTypeName!=null)
    var CHECK_#(f.varName.toUpperCase())_DATA = [[${enum.toArray('#(f.checkField.enumTypeName)')}]];
    #end
    #if(f.checkField.varName!=null)
    var CHECK_#(f.varName.toUpperCase())_DATA = [[${#(f.checkField.varName)}]];
    #end
    #else if(f.typeName.equals("select_box"))
    #if(f.selectField.dictCode!=null)
    var SELECT_#(f.varName.toUpperCase())_DATA = [[${dict.toArray('#(f.selectField.dictCode)')}]];
    #end
    #if(f.selectField.enumTypeName!=null)
    var SELECT_#(f.varName.toUpperCase())_DATA = [[${enum.toArray('#(f.selectField.enumTypeName)')}]];
    #end
    #if(f.selectField.varName!=null)
    var SELECT_#(f.varName.toUpperCase())_DATA = [[${#(f.selectField.varName)}]];
    #end
    #end
    #end
    var AUTH_PREFIX="#(authPrefix)";

    #for(v:jsVariables)
    // #(v.note)
    var #(v.name) = #(v.value) ;
    #end

</script>

<script th:src="'#(extURI)?'+${cacheKey}"></script>
<script th:src="'#(jsURI)?'+${cacheKey}"></script>

</body>
</html>
